#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/esm/bin/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/esm/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules:/workspace/eliza-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/esm/bin/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/esm/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/dist/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules/uuid/node_modules:/workspace/eliza-client/node_modules/.pnpm/uuid@11.0.3/node_modules:/workspace/eliza-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/esm/bin/uuid" "$@"
else
  exec node  "$basedir/../../dist/esm/bin/uuid" "$@"
fi
