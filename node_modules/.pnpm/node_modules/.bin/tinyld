#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/bin/node_modules:/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/node_modules:/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules:/workspace/eliza-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/bin/node_modules:/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/node_modules:/workspace/eliza-client/node_modules/.pnpm/tinyld@1.3.4/node_modules:/workspace/eliza-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tinyld/bin/tinyld.js" "$@"
else
  exec node  "$basedir/../tinyld/bin/tinyld.js" "$@"
fi
